{
  "permissions": {
    "allow": [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "WebFetch",
      "WebSearch",
      "Task",
      "NotebookEdit",
      "mcp__ide__getDiagnostics",
      "mcp__chrome-devtools__*"
    ],
    "deny": []
  },
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["@anthropic/claude-mcp-devtools@latest"]
    }
  }
}
